generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id             Int      @id @default(autoincrement())
  email          String   @unique
  password_hash  String
  nombre         String
  apellido       String
  fecha_creacion DateTime @default(now())
}
